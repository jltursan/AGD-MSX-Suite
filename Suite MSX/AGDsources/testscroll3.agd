EVENT PLAYER
SPAWN 1 1
 LET M = 0
 IF KEY UP
  LET M = 1
  IF CANGOUP
   SPRITEUP
  ENDIF
 ENDIF
 IF KEY DOWN
  LET M = 1
  IF CANGODOWN
   SPRITEDOWN
  ENDIF
 ENDIF
 IF KEY LEFT
  LET M = 1
  IF CANGOLEFT
   SPRITELEFT
  ENDIF
 ENDIF
 IF KEY RIGHT
  LET M = 1
  IF CANGORIGHT
   SPRITERIGHT
  ENDIF
 ENDIF
 IF M > 0
  ANIMATE
 ENDIF
 IF KEY FIRE
   IF Q > 5
     LASER 1
     LET Q = 0
   ENDIF
 ENDIF

EVENT SPRITETYPE1
    LET X = RIGHTEDGE                   ; move it to the right edge.
    ADD 8 TO X                          ; move centre of sprite to the very edge.
    IF H = 0
        LET H = BOTTOMEDGE
        ADD 9 TO H
    ENDIF
    LET Y = H                           ; set the height.
    NEWPARTICLE 10
ADD I TO H                              ; adjust height by increment.

IF P = 0                                ; no pixels left to plot.
    IF H > BOTTOMEDGE
        GETRANDOM 3                     ; up or level.
        SUBTRACT 2 FROM RND
    ELSE
        LET K = BOTTOMEDGE
        SUBTRACT 32 FROM K
        IF H < K                        ; too close to top?
            GETRANDOM 3                 ; down or level.
        ELSE
            GETRANDOM 5                 ; up, down or level.
            SUBTRACT 2 FROM RND
        ENDIF
    ENDIF
    LET I = RND                         ; put y increment/decrement in I.
    LET P = 8
ENDIF

SUBTRACT 1 FROM P

LET K = TOPEDGE
ADD 32 TO K
IF H < K                                ; too close to top?
    LET I = 2                           ; force landscape down.
ENDIF

